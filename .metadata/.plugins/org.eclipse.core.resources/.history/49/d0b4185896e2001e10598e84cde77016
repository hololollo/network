package sec1;

import java.net.InetAddress;
import java.net.UnknownHostException;

public class NetworkExam3 {
	public static void main(String[] args) {
		//본인 컴퓨터의 ip주소와 컴퓨터이름 출력
		
		try {
			InetAddress a =  InetAddress.getLocalHost();
			System.out.println(a.getHostAddress()); // ip주소 출력
			System.out.println(a.getHostName()); // 컴퓨터 이름 출력
			InetAddress[] arr = InetAddress.getAllByName("www.google.com");
			
			for(InetAddress b : arr) {
				System.out.println(b.getHostAddress()); 
			}
		} catch (UnknownHostException e) {
			e.printStackTrace();
		}
		
		
		//www.google.com의 연결 IP주소를 출력
	}

}
